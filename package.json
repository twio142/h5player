{
  "name": "h5player",
  "version": "3.8.0",
  "description": "h5player tampermonkey",
  "author": "xxxily",
  "private": true,
  "scripts": {
    "rollup": "rollup -w -c config/rollup.config.js",
    "h5player": "cross-env MODE=prod  PROJECT_NAME=h5player rollup -w -c config/rollup.config.js",
    "h5playerUI": "cross-env MODE=prod  PROJECT_NAME=h5playerUI rollup -w -c config/rollup.config.js",
    "start": "yarn run h5playerUI & yarn run h5player",
    "jsonEditor": "cross-env MODE=prod  PROJECT_NAME=jsonEditor rollup -c config/rollup.config.js",
    "server": "node bin/staticService/index.js",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:release": "node bin/docs/release.js",
    "giteeDocs:dev": "cross-env BASEPATH='/h5player/' vitepress dev docs",
    "giteeDocs:build": "cross-env BASEPATH='/h5player/' vitepress build docs",
    "giteeDocs:preview": "cross-env BASEPATH='/h5player/' vitepress preview docs",
    "giteeDocs:release": "cross-env BASEPATH='/h5player/' DOCSTYPE='gitee' node bin/docs/release.js"
  },
  "dependencies": {
    "@shoelace-style/shoelace": "^2.12.0",
    "@vue/server-renderer": "^3.4.21",
    "local-storage-proxy": "^4.0.3",
    "lodash": "^4.17.21",
    "vue": "^3.4.21"
  },
  "type": "module",
  "license": "GPL",
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-dynamic-import-vars": "^2.1.2",
    "@rollup/plugin-image": "^3.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "eslint": "^8.0.1",
    "eslint-config-standard": "latest",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "morgan": "^1.10.0",
    "rollup": "^4.9.6",
    "rollup-plugin-import-css": "^3.4.0",
    "rollup-plugin-uglify": "^6.0.4",
    "vitepress": "^1.0.2"
  },
  "engines": {
    "node": ">= 16.0.0",
    "npm": ">= 7.0.0"
  },
  "volta": {
    "node": "20.11.0",
    "yarn": "3.7.0"
  }
}
